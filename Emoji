local whitelisted = {
  [2266148679] = true;
  [95199610] = true;
  [3359416636] = true;
  [9796712829] = true;
}

function Emoji()
	for _,v in pairs(game:GetService('Players'):GetChildren()) do
		if whitelisted[v.UserId] then
			if v.Character then
				if v.Character.Parent.Name == 'Players' then
					v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]'..v.DisplayName)
				end
			end
		end
	end
end
local success,err = pcall(Emoji)
return whitelisted
