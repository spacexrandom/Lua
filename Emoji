local function Emoji_System()
    for i,v in pairs(game.Players:GetChildren()) do
        if Emoji[v.UserId] then
            if v.Character then
                if v.Character.Parent.Name == 'Players' then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]'..v.DisplayName)
                end
            end
        else
            if v.Character then
                if not v.Character.LeftUpperLeg:FindFirstChild("OriginalSize") then
                    v.Character:FindFirstChild("Humanoid").DisplayName = "[üëª]"..v.DisplayName
                end
            end
        end
    end
end
local success, err = pcall(Emoji_System)
return Emoji
